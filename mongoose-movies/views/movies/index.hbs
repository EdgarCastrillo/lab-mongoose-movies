<h2>Movies Page</h2>

<ul>
  {{#each movies}}
  <form action="movies/{{id}}/delete" method="POST">
    <li>
      <a href="/movies/{{id}}">{{title}}</a>
      <button type="submit">Delete</button>
      <a href="/movies/{{id}}/edit">Edit</a>
    </li>
  </form>
  {{/each}}
</ul>


<a href="/movies/new">New</a>